    "plugins": [
        [
            //放置污染全局作用域
            //提高代码重用性，缩小体积
            "@babel/plugin-transform-runtime",
            {
                "obsoluteRuntime": false,
                "corejs": 2,
                "helpers": true,
                "regenerator": true,
                "useESMidules": false
            }
        ]
    ]
}